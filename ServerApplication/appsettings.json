{
  "serviceAddress": "localhost:8090",
  "serviceName": "MouseControlService",

  "ConnectionStrings": {
    // Local
    "DefaultConnection": "FileName=mouseManager.db"
  },

  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "archiveFileName": "log.txt"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "log.txt"
      }
    ]
  },

  "WhatsApp": {
    "token": "token",
    "instance": "instance"
  },
  
  "Mail": {
    "login": "login",
    "password": "password"
  }
}
